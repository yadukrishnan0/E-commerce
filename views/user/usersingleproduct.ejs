                <p class="ratedStar">★</p>
                <p class="ratedStar">★</p>
                <p class="ratedStar">★</p>
                <p class="ratedStar">★</p>
                <p class="unRatedStar">★</p>
              </div>
              <div class="price">
                <%const result = (product.price * product.discount) / 100;
                            const discountedPrice = Math.ceil(product.price - result);%>
                <p class="lPrice">₹<%=discountedPrice%></p>
                <p class="fPrice">₹<%=product.price%></p>
                <p class="stock text-success"><%=product.discount%>%</p>
              </div>
              <p class="stock text-success">Stock:<%=product.stock%></p>
